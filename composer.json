{
    "type": "project",
    "license": "proprietary",
    "name": "symfonycasts/symfony6_upgrade",
    "description": "Upgrade to Symfony 6 course code",
    "require": {
        "php": "^8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "babdev/pagerfanta-bundle": "^3.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3.7",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.0",
        "knplabs/knp-markdown-bundle": "^1.8",
        "knplabs/knp-time-bundle": "^1.18",
        "pagerfanta/doctrine-orm-adapter": "^3.8",
        "pagerfanta/twig": "^3.8",
        "sentry/sentry-symfony": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^2.1",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-mbstring": "^1.28",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/routing": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/stopwatch": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/ux-chartjs": "^2.0",
        "symfony/validator": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "6.0.*",
        "symfonycasts/verify-email-bundle": "^1.7",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.3",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.18.12",
        "symfony/debug-bundle": "6.0.*",
        "symfony/maker-bundle": "^1.15",
        "symfony/var-dumper": "6.0.*",
        "symfony/web-profiler-bundle": "6.0.*",
        "zenstruck/foundry": "^1.16"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
